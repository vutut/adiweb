!!! 5
%html{ :lang => I18n.locale}
  %head
    %meta{ :charset => "utf-8"}
    %meta{ "http-equiv" => "X-UA-Compatible", :content => "IE=edge,chrome=1"}
    %meta{ :name => "description", :content => I18n.t(@description)}
    %meta{ :name => "viewport", :content => "width=device-width, initial-scale=1, shrink-to-fit=no"}
    %meta{:content => "#2F3BA2", :name => "theme-color"}
    %link{ href: "https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css", rel: "stylesheet", integrity: "sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO", crossorigin: "anonymous"}
    %link{ :rel => "stylesheet", :href => url('/styles.css'), :type => 'text/css'}
    %link{:href => "https://fonts.googleapis.com/css?family=Montserrat|Noto+Sans|Cabin&subset=latin-ext", :rel => "stylesheet"}
    %title
      = I18n.t(@title)
  %body
    %nav.navbar.pl-4.navbar-expand-lg.navbar-dark.bg-primary.sticky-top.pb-0.pt-3
      %a.navbar-brand.mb-3{:href => url(@home), :itemprop => "logo"}
        %img{:alt => "adilogo", :src => "data:image/png;base64,", :width => "140"}
      %button.navbar-toggler{"aria-controls" => "navbar", "aria-expanded" => "false", "aria-label" => "Toggle navigation", "data-target" => "#myNavbar", "data-toggle" => "collapse", :type => "button"}
        %span.navbar-toggler-icon
      #navbarTogglerDemo02.collapse.navbar-collapse
        %ul.navbar-nav.mr-auto.mt-2.mt-lg-0
      #myNavbar.collapse.navbar-collapse
        %ul.navbar-nav.ml-auto
          %li.hvr-underline-reveal.nav-item.pb-3
            %a.nav-link{:href => url(@home)}Home
          %li.hvr-underline-reveal.nav-item.pb-3
            %a.nav-link{:href => url(@about)}
              = I18n.t('about')
          %li.hvr-underline-reveal.nav-item.pb-3
            %a.nav-link{:href => url(@products)}
              = I18n.t('products')
          %li.hvr-underline-reveal.nav-item.pb-3
            %a.nav-link{:href => url(@sale)}
              = I18n.t('sale')
          %li.hvr-underline-reveal.nav-item.pb-3
            %a.nav-link{:href => url(@support)}
              = I18n.t('support')
          %li.hvr-underline-reveal.nav-item.pb-3
            %a.nav-link{:href => url(@contact)}
              = I18n.t('contact')
          %li.nav-item.pb-3
            %a.nav-link.lang{:href => url(@switchtoen)}
              =@otherlang









    %script{:crossorigin => "anonymous", :integrity => "sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo", :src => "https://code.jquery.com/jquery-3.3.1.slim.min.js"}
    %script{:crossorigin => "anonymous", :integrity => "sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49", :src => "https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"}
    %script{:crossorigin => "anonymous", :integrity => "sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy", :src => "https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"}
    =yield

    %footer.mdl-mega-footer
      .mdl-mega-footer--middle-section
        .mdl-mega-footer--drop-down-section.mx-auto
          %h1.mdl-mega-footer--heading= I18n.t('about')
          %ul.mdl-mega-footer--link-list
            %li
              %a{:href => @about+'#basic'}=I18n.t('basic')
            %li
              %a{:href => @about+'#offer'}=I18n.t('offer')
            %li
              %a{:href => @about+'#history'}=I18n.t('history')
    %footer.mdl-mini-footer
